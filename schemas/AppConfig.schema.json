{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/AppConfig",
    "definitions": {
        "AppConfig": {
            "properties": {
                "loginTimestamp": {
                    "type": "integer"
                },
                "colorTheme": {
                    "type": "integer"
                },
                "mode": {
                    "type": "integer"
                },
                "connection": {
                    "$ref": "#/definitions/PbApp.Connection",
                    "additionalProperties": false
                },
                "savedConnections": {
                    "additionalProperties": {
                        "$ref": "#/definitions/PbApp.Connection",
                        "additionalProperties": false
                    },
                    "type": "object"
                },
                "activeTabs": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "triggers": {
                    "items": {
                        "$ref": "#/definitions/PbApp.Trigger"
                    },
                    "additionalProperties": false,
                    "type": "array"
                },
                "customPlots": {
                    "items": {
                        "$ref": "#/definitions/PbApp.CustomPlot"
                    },
                    "additionalProperties": false,
                    "type": "array"
                },
                "filesPaths": {
                    "additionalProperties": {
                        "type": "string"
                    },
                    "type": "object"
                },
                "stringCache": {
                    "additionalProperties": {
                        "type": "string"
                    },
                    "type": "object"
                },
                "boolCache": {
                    "additionalProperties": {
                        "type": "boolean"
                    },
                    "type": "object"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "App Config"
        },
        "PbApp.Axis": {
            "properties": {
                "name": {
                    "type": "string"
                },
                "items": {
                    "items": {
                        "$ref": "#/definitions/PbApp.AxisItem"
                    },
                    "additionalProperties": false,
                    "type": "array"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Axis"
        },
        "PbApp.AxisItem": {
            "properties": {
                "message": {
                    "type": "string"
                },
                "signal": {
                    "type": "string"
                },
                "isEnum": {
                    "type": "boolean"
                },
                "color": {
                    "type": "integer"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Axis Item"
        },
        "PbApp.Connection": {
            "properties": {
                "ip": {
                    "type": "string"
                },
                "port": {
                    "type": "string"
                },
                "mode": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Connection"
        },
        "PbApp.CustomPlot": {
            "properties": {
                "name": {
                    "type": "string"
                },
                "axes": {
                    "additionalProperties": {
                        "$ref": "#/definitions/PbApp.Axis",
                        "additionalProperties": false
                    },
                    "type": "object"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Custom Plot"
        },
        "PbApp.Trigger": {
            "properties": {
                "id": {
                    "type": "string"
                },
                "message": {
                    "type": "string"
                },
                "signal": {
                    "type": "string"
                },
                "comparator": {
                    "type": "integer"
                },
                "value": {
                    "type": "number"
                },
                "color": {
                    "type": "integer"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Trigger"
        }
    }
}